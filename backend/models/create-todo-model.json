{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "todoItem",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "dueDate": {
      "type": "string"
    },
    "userId": {
      "type": "string"
    },
    "todoId": {
      "type": "string"
    },
    "createdAt": {
      "type": "string"
    },    
    "done": {
      "type": "boolean"
    },
    "attachmentUrl": {
      "type": "boolean"
    }
  },
  "required": [
    "name",
    "dueDate"
  ],
  "additionalProperties": false 
}
